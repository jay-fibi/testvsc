{
    "appName": "My Application",
    "version": "1.0.0",
    "description": "A sample web application configuration",
    "environment": "development",
    "settings": {
        "debug": true,
        "logLevel": "info",
        "maxRetries": 3,
        "timeout": 5000
    },
    "api": {
        "baseUrl": "https://api.example.com",
        "version": "v1",
        "endpoints": {
            "users": "/users",
            "products": "/products",
            "orders": "/orders"
        }
    },
    "database": {
        "host": "localhost",
        "port": 5432,
        "name": "myapp_db",
        "poolSize": 10
    },
    "features": {
        "enableAnalytics": true,
        "enableNotifications": true,
        "enableDarkMode": false
    },
    "metadata": {
        "author": "Developer",
        "createdAt": "2026-01-22",
        "lastUpdated": "2026-01-22"
    }
}
